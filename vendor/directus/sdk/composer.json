{
    "name": "directus/sdk",
    "description": "Directus SDK for PHP",
    "keywords": ["directus", "sdk"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "directus/config": "^1.0.0",
        "directus/database": "^1.0.0",
        "directus/permissions": "^1.0.0",
        "directus/filesystem": "^1.0.0",
        "directus/hash": "^1.0.0",
        "directus/hook": "^1.0.0",
        "directus/utils": "^1.0.0",
        "guzzlehttp/guzzle": "~5.3.0 | ^6.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Directus\\SDK\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "repositories": [{
        "type": "git",
        "url": "https://github.com/wellingguzman/zend-db"
    }]
}
